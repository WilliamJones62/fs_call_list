<p id="notice"><%= notice %></p>

<h2> All Active Customers </h2>
<table id="not_called"  class="stripe" class="mytable" class="display" cellspacing="0">
  <thead>
    <tr>
      <th> Customer Code </th>
      <th> Name </th>
      <th> Contact Method </th>
      <th> Contact </th>
    </tr>
  </thead>
  <tbody>
  <% @report_list.each do |r| %>
    <tr>
      <% @found = false %>
      <td><%= r.custcode %></td>
      <% @call_list.each do |c| %>
        <% if r.custcode == c.custcode %>
          <td><%= c.custname %></td>
          <td><%= c.contact_method %></td>
          <td><%= c.contact %></td>
          <% @found = true %>
        <% end %>
      <% end %>
      <% if !@found %>
        <td> </td>
        <td> </td>
        <td> </td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>
