<p id="notice"><%= notice %></p>

<h2> Call Lists With No Active Customer </h2>
<table id="not_called"  class="stripe" class="mytable" class="display" cellspacing="0">
  <thead>
    <tr>
      <th> Customer </th>
      <th> Delete call list </th>
    </tr>
  </thead>
  <tbody>
  <% @missing_customer.each do |r| %>
    <tr>
      <td><%= r.custcode %></td>
      <td><%= link_to 'Delete', r, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>
