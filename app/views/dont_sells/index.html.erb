<p id="notice"><%= notice %></p>

<h1>Dont Sells</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Part</th>
      <th>Dontcalls start</th>
      <th>Dontcalls end</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dont_sells.each do |dont_sell| %>
      <tr>
        <td><%= dont_sell.customer %></td>
        <td><%= dont_sell.part %></td>
        <td><%= dont_sell.dontcalls_start %></td>
        <td><%= dont_sell.dontcalls_end %></td>
        <td><%= link_to 'Show', dont_sell %></td>
        <td><%= link_to 'Edit', edit_dont_sell_path(dont_sell) %></td>
        <td><%= link_to 'Destroy', dont_sell, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

