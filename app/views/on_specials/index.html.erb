<p id="notice"><%= notice %></p>

<h1>On Specials</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Part</th>
      <th>Onspecials start</th>
      <th>Onspecials end</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @on_specials.each do |on_special| %>
      <tr>
        <td><%= on_special.customer %></td>
        <td><%= on_special.part %></td>
        <td><%= on_special.onspecials_start %></td>
        <td><%= on_special.onspecials_end %></td>
        <td><%= link_to 'Show', on_special %></td>
        <td><%= link_to 'Edit', edit_on_special_path(on_special) %></td>
        <td><%= link_to 'Destroy', on_special, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New On Special', new_on_special_path %>
